<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveSuite">
    <test name="PositiveTests">
        <classes>
            <class name="Registration" >
                <methods>
                    <include name="testSuccessRegistrationFromSignUpPage"></include>
                    <include name="testSuccessRegistrationFromSignInPage"></include>
                    <include name="testSuccessEmailRegistration"></include>
                </methods>
            </class>
            <class name="IntroBooking" >
                <methods>
                    <include name="testBookingAsFistTimeVisitor"></include>
                </methods>
            </class>
            <class name="Booking" >
                <methods>
                    <include name="testBookingAsGuest"></include>
                    <include name="testBookingAsMember"></include>
                    <include name="testSuccessEmailBooking"></include>
                    <include name="testBookingWithRegistration"></include>
                </methods>
            </class>
            <class name="LMDBooking" >
                <methods>
                    <include name="testLMDBookingFromHomePage"></include>
                    <include name="testLMDBookingFromSpaDealsPage"></include>
                    <include name="testLMDBookingFromLocationPage"></include>
                </methods>
            </class>
            <class name="SignIn" >
                <methods>
                    <include name="testSuccessLogInFromSignInPopup"></include>
                    <include name="testSuccessLogInFromSignInPage"></include>
                    <include name="testSuccessUpdateContactInfo"></include>
                    <include name="testSuccessChangePassword"></include>
                    <include name="testSuccessEmailChangePassword"></include>
                </methods>
            </class>
            <class name="adminPanel.CustomersAction" >
                <methods>
                    <include name="testSendingResetPassword"></include>
                    <include name="testSuccessResetPassEmail"></include>
                    <include name="testSuccessResetNewPassword"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>

